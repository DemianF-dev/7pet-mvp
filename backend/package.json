{
    "name": "7pet-backend",
    "version": "0.1.2-rc3",
    "description": "Backend for 7Pet MVP",
    "main": "src/index.ts",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "build": "prisma generate && tsc",
        "start": "node dist/index.js",
        "seed": "ts-node prisma/seed.ts",
        "vercel-build": "rm -rf dist && prisma generate && tsc",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:ci": "jest --runInBand --detectOpenHandles",
        "typecheck": "tsc --noEmit",
        "rls:apply": "npx tsx scripts/configure_all_rls.ts",
        "security:check": "npm audit",
        "security:fix": "npm audit fix",
        "security:fix:force": "npm audit fix --force",
        "security:report": "npm audit --json > security-audit-report.json",
        "lint": "eslint src",
        "version:generate": "node ../scripts/generate-version.js"
    },
    "dependencies": {
        "@ai-sdk/google": "^3.0.18",
        "@ai-sdk/openai": "^3.0.20",
        "@googlemaps/google-maps-services-js": "^3.4.2",
        "@prisma/adapter-neon": "^7.2.0",
        "@prisma/adapter-pg": "^7.2.0",
        "@prisma/client": "^7.2.0",
        "@supabase/supabase-js": "^2.90.1",
        "ai": "^3.4.0",
        "axios": "^1.13.2",
        "bcryptjs": "^3.0.3",
        "compression": "^1.8.1",
        "cors": "^2.8.5",
        "date-fns": "^4.1.0",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-rate-limit": "^8.2.1",
        "google-auth-library": "^10.5.0",
        "helmet": "^8.1.0",
        "jsonwebtoken": "^9.0.3",
        "multer": "^1.4.5-lts.1",
        "pg": "^8.17.2",
        "pino": "^10.2.1",
        "socket.io": "^4.8.3",
        "web-push": "^3.6.7",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/bcrypt": "^6.0.0",
        "@types/bcryptjs": "^2.4.6",
        "@types/compression": "^1.8.1",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/express-rate-limit": "^5.1.3",
        "@types/jest": "^30.0.0",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.12.12",
        "@types/pg": "^8.16.0",
        "@types/supertest": "^6.0.3",
        "@types/web-push": "^3.6.3",
        "@typescript-eslint/eslint-plugin": "^8.54.0",
        "@typescript-eslint/parser": "^8.54.0",
        "eslint": "^9.39.2",
        "jest": "^30.2.0",
        "jest-mock-extended": "^4.0.0",
        "pino-pretty": "^13.1.3",
        "prisma": "^7.2.0",
        "supertest": "^7.1.4",
        "ts-jest": "^29.4.6",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.4.5"
    },
    "prisma": {
        "seed": "ts-node prisma/seed.ts"
    }
}
