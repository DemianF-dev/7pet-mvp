{
    "name": "7pet-backend",
    "version": "0.1.0-beta",
    "description": "Backend for 7Pet MVP",
    "main": "src/index.ts",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "build": "echo 'Build step skipped - Vercel uses source files directly'",
        "start": "node dist/index.js",
        "seed": "ts-node prisma/seed.ts",
        "vercel-build": "prisma generate && echo 'Skipping TypeScript compilation - Vercel will use source files directly'",
        "postinstall": "prisma generate",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch",
        "rls:apply": "npx tsx scripts/configure_all_rls.ts",
        "security:check": "npm audit",
        "security:fix": "npm audit fix",
        "security:fix:force": "npm audit fix --force",
        "security:report": "npm audit --json > security-audit-report.json",
        "version:generate": "node ../scripts/generate-version.js"
    },
    "dependencies": {
        "@googlemaps/google-maps-services-js": "^3.4.2",
        "@prisma/client": "^6.0.0",
        "@types/multer": "^2.0.0",
        "axios": "^1.13.2",
        "bcrypt": "^6.0.0",
        "bcryptjs": "^3.0.3",
        "compression": "^1.8.1",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-rate-limit": "^8.2.1",
        "google-auth-library": "^10.5.0",
        "helmet": "^8.1.0",
        "jsonwebtoken": "^9.0.3",
        "multer": "^2.0.2",
        "socket.io": "^4.8.3",
        "web-push": "^3.6.7",
        "winston": "^3.19.0",
        "zod": "^4.2.1"
    },
    "devDependencies": {
        "@types/bcrypt": "^6.0.0",
        "@types/bcryptjs": "^2.4.6",
        "@types/compression": "^1.8.1",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/express-rate-limit": "^5.1.3",
        "@types/jest": "^30.0.0",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "^20.12.12",
        "@types/supertest": "^6.0.3",
        "@types/web-push": "^3.6.3",
        "jest": "^30.2.0",
        "jest-mock-extended": "^4.0.0",
        "prisma": "^6.0.0",
        "supertest": "^7.1.4",
        "ts-jest": "^29.4.6",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.4.5"
    },
    "prisma": {
        "seed": "ts-node prisma/seed.ts"
    }
}
