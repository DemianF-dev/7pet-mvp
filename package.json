{
  "name": "7pet-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate --schema=backend/prisma/schema.prisma",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && npm install && prisma generate",
    "build:frontend": "cd frontend && npm install && npm run build && cp ../VERSION.json dist/",
    "test:all": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.4.2",
    "@prisma/client": "^6.0.0",
    "prisma": "^6.0.0",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^8.2.1",
    "google-auth-library": "^10.5.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "winston": "^3.19.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/express": "^4.17.21",
    "typescript": "^5.4.5",
    "ts-node": "^10.9.2"
  }
}